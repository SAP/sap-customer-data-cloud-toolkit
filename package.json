    "cypress:run": "cypress run",
    "cypress:ci": "concurrently \"cypress run\" --kill-others \"react-scripts start\" --success first",
    "postbuild": "node ./scripts/postbuild.js",
    "test": "CI=true react-scripts test --collect-coverage --watch=all",
    "eject": "react-scripts eject"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@ui5|lit-html|@babel)).*\\.js$"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-scripts": "5.0.0",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/instrument-cra": "^1.4.0",
    "babel-plugin-istanbul": "^6.1.1",
    "concurrently": "^7.4.0",
    "cypress": "^10.10.0",
    "prettier": "2.7.1"
  }
}