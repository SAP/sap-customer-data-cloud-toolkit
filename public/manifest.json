{
  "manifest_version": 3,
  "name": "SAP Customer Data Cloud Toolbox",
  "version": "1.3.1",
  "description": "Add extra functionality to SAP Customer Data Cloud Toolbox console",
  "permissions": ["storage"],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": ["https://console.gigya.com/*"],
      "all_frames": true,
      "run_at": "document_end",
      "js": [],
      "css": [],
      "media": []
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [],
      "matches": ["<all_urls>"]
    }
  ]
}
