{
  "manifest_version": 3,
  "name": "SAP CDC Tools",
  "version": "0.2.0",
  "description": "Add extra functionality to SAP CDC console",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": [
        "https://console.gigya.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "static/js/main.66b80c7d.js"
      ],
      "css": [
        "static/css/main.155bff51.css"
      ],
      "media": []
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "static/css/main.155bff51.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}